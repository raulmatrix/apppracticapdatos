<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DatosEstudiante">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Datos Estudiante"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtnom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Nombres"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtape2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Apellidos"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtnom2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtcodes2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="CodEst"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtape2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtfnac2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Fecha nac"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtcodes2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtsexo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Sexo"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtfnac2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtdpto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="departamento"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtsexo2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtcarr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Carrera"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtdpto2"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/txtdir2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Direccion"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txtcarr2"
        tools:layout_editor_absoluteX="56dp" />
</androidx.constraintlayout.widget.ConstraintLayout>